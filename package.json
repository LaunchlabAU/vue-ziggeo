{
  "name": "vue-ziggeo",
  "description": "VueJS integration for Ziggeo's video recording/playback API",
  "version": "0.0.1",
  "main": "./build/index",
  "author": "Ziggeo Inc <support@ziggeo.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "prebuild": "rimraf ./build/* && cp node_modules/ziggeo-client-sdk/build/*.swf build && cp node_modules/ziggeo-client-sdk/build/*.png build",
    "rebuild-demo": "npm run build && rm vue-ziggeo-0.0.1.tgz && npm pack && cd demo/ && rimraf node_modules/vue-ziggeo/ && npm i ../vue-ziggeo-0.0.1.tgz"
  },
  "files": ["build"],
  "peerDependencies": {
    "vue": ">=2.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "vue": "^2.5.11",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "ziggeo-client-sdk": "^0.0.29"
  }
}
